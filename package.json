{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "build": "npm run tsc && npm run css",
    "mkdir": "mkdirp ./dist",
    "tsc": "./node_modules/.bin/tsc -p .",
    "css": "npm run mkdir && stylus ./css -o ./dist",
    "start": "npm run build && electron ./bin/main"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
