    "@types/bluebird": "^3.0.36",
    "@types/electron": "^1.4.30",
    "@types/history": "2.0.39",
    "@types/lodash": "^4.14.44",
    "@types/material-ui": "^0.16.44",
    "@types/node-uuid": "^0.0.28",
    "@types/react": "^0.14.55",
    "@types/react-dom": "^0.14.19",
    "@types/react-router": "2.0.38",
    "electron": "^1.4.1",
    "mkdirp": "^0.5.1",
    "stylus": "^0.54.5",
    "typescript": "^2.1.4"
  },
  "dependencies": {
    "axios": "^0.15.3",
